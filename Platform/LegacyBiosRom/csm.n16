%macro TTY_BYTE 1
	mov		al,%1
	mov		ah,al
	and		al,0xf0
	shr		al,4
	cmp		al,0x9
	jle		%%b1
;	sub		al,0xA
	add 	al,'A'- 0x30 - 0xA
