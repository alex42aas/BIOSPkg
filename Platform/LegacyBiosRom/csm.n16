  push  bp
  mov   bp, sp

  push  mes_trap
  call  SendTxt

  push  ax
  mov   ax, word [ss:bp+4]
  push  ax
  call  SendWord
  push  ':'
  call  SendChar
  mov   ax, word [ss:bp+2]
  push  ax
  call  SendWord
  push  ' '
  call  SendChar
