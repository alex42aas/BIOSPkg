  	mov   dx, word [cs:secd_bmaster]
  	add   dx, 2       ; Secondary Bus Master IDE status register
  	in    al, dx
  	or    al, 0x84
  	out   dx, al

	mov		  dx, word [cs:prim_cmd_stat]
	in		  al, dx					        ; clear interrupt request
	mov		  dx, word [cs:secd_cmd_stat]
	in		  al, dx					        ; clear interrupt request

