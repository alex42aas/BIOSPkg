ALIGN 32
DefaultInt:
  sti

  push  bp			    ;
  mov   bp, sp			    ;

  push  ds			    ;
  pusha				    ;
  push	ecx			    ;

;   COM-: ("Dummy %04x:%04x %04x", segment, offset, data);
  push  mes_dummy		    ;
  call  SendTxt			    ; "Dummy "

  mov   si, word [ss:bp + 2]	    ; offset
  mov   ax, word [ss:bp + 4]	    ; segment

  mov	ds, ax			    ;
  push	ax			    ;
  call  SendWord		    ;  : 
  call	SendColon		    ;
  push	si			    ;
  call  SendWord		    ;  : 
  call	SendSpace		    ;
  mov   cx, word [si - 2]	    ;

  push	cx			    ;
  push  cx			    ;
  call  SendWord		    ;   : ,  INT xx (0xcdXX)

  push  mes_crlf		    ;
  call  SendTxt			    ;

  ; pause ~ 1sec
  mov   ecx, 0x00100000		    ;
delay_in_dummy:
  in    al, iowait		    ;
  loop  delay_in_dummy, ecx	    ;

  pop	cx			    ;

  cmp	cx, 0x10cd		    ; INT 10?
  je	dummy_2			    ;  -  
;	----------------------------
;      int10    COM-:
;
;VIDEO - WRITE STRING (AT and later,EGA)
;
;AH = 13h
;AL = write mode
;
;bit 0:
;Update cursor after writing
;
;bit 1:
;String contains alternating characters and attributes
;
;bits 2-7:
;Reserved (0).
;BH = page number.
;BL = attribute if string contains only characters.
;CX = number of characters in string.
;DH,DL = row,column at which to start writing.
;ES:BP -> string to write
;
;Return: Nothing
;
  mov   al, 1			    ; al - mode
  mov   ah, 0x13		    ; output string
  mov   bh, 0x00		    ; page #
  mov   bl, 0x07		    ; color
  mov	dx, 0x0101		    ; (x,y)
  mov	cx, 5			    ; strlen("dummy");
  push	cs			    ; 
  pop	es			    ; string segment
  mov	bp, mes_dummy		    ; string offset
  int   0x10			    ;
dummy_loop:
  jmp	short	dummy_loop	    ;    

dummy_2:
  pop	ecx			    ;
  popa				    ;
  pop   ds			    ;
  pop   bp			    ;
;	----------------------------
;    INT10,   ,  :
  push  bp			    ;
  mov   bp, sp			    ;
  push  ax			    ;
  mov   ax, word [ss:bp + 6]	    ;
  or    ax, 0x0001		    ; set carry
  mov   word [ss:bp+6], ax	    ;
  pop   ax			    ;
  pop   bp			    ;

  iret				    ;

