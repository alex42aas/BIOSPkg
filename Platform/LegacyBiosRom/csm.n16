  push  ':'
  call  SendChar

  push  di
  call  SendWord      ; di
  push  ' '
  call  SendChar

  push  eax
  push  es
  push  si
  mov   ax, word [ss:bp+4]
  mov   es, ax
  mov   si, word [ss:bp+2]
  mov   eax, dword [es:si]
