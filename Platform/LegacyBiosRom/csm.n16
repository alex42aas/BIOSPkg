  cmp		ax, Legacy16RetrieveLastBootDevice
	jne		not_Legacy16RetrieveLastBootDevice

;	mov		ax, 0x0000				; return EFI_SUCCESS
	mov		ax, 0x8000				; return EFI_TBD
	retf

