ALIGN 32
Int03:	
	mov	cx, 3
int03_entry:
  mov	bp, sp
	push	cx
  	push  	mes_int
  	call  	SendTxt
	call	SendByte	    ; "^IntXX"
	call	crlf

	mov	cx, 8
int03_l1:
	mov   ax, word [ss:bp]	
	push	ax
	call	SendWord
	push  ' '
	call  SendChar
	add	bp, 2
	mov   ax, word [ss:bp]	
	push	ax
	call	SendWord
	add	bp, 2
	call	crlf

	dec	cx
	jne	int03_l1

int03_loop:
	jmp	int03_loop

crlf:
	push	mes_crlf
	call	SendTxt
	ret

ALIGN 32
Int04:	
	mov	cx, 4
	jmp	int03_entry
ALIGN 32
Int06:	
	mov	cx, 6
	jmp	int03_entry
ALIGN 32
Int07:	
	mov	cx, 7
	jmp	int03_entry

